{
  "version": "1.3.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://www.pathofexile.com/trade*"
      ],
      "js": [
        "assets/vendor.js",
        "assets/better-trading.js"
      ],
      "css": [
        "assets/vendor.css",
        "assets/better-trading.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "storage",
    "*://poe.ninja/*"
  ],
  "web_accessible_resources": [
    "assets/images/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "name": "Better PathOfExile Trading",
  "description": ""
}