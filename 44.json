{
  "background": {
    "page": "Coupert.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/browsericon-off-19.png",
      "38": "icon/browsericon-off-38.png"
    },
    "default_title": "Coupert",
    "default_popup": "popover.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content.js"
      ],
      "match_about_blank": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    },
    {
      "all_frames": false,
      "js": [
        "guide.js"
      ],
      "match_about_blank": false,
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "default_locale": "en",
  "description": "__MSG_app_description__",
  "icons": {
    "16": "icon/icon-16.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  },
  "manifest_version": 2,
  "name": "__MSG_app_name__",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "alarms",
    "http://*/*",
    "https://*/*",
    "activeTab"
  ],
  "version": "6.0.40",
  "web_accessible_resources": [
    "image/*",
    "css/*",
    "template/*",
    "font/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "appledev@soarinfotech.com"
    }
  }
}