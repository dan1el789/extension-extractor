{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Converter Suite & Custom Web Search",
  "short_name": "Pdf Converter",
  "icons": {
    "128": "icons/favicon_coloured.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://convertersuite.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "description": "Add the Converter Suite & Custom Web Search extension, then click the icon on the toolbar to easily convert files & access Yahoo\u00ae search.",
  "version": "1.0.0.6",
  "manifest_version": 2,
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "128": "icons/icon128.png"
    },
    "default_title": "Converter Suite & Custom Web Search"
  },
  "permissions": [
    "*://convertersuite.com/",
    "tabs",
    "storage"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "web search by Yahoo",
      "keyword": "Pdf Converter",
      "favicon_url": "icons/icon128.png",
      "search_url": "https://search.yahoo.com/yhs/search?hspart=mnet&hsimp=yhs-001&type=type9045647-spa-6246-84501&param1=6246&param2=84501&p={searchTerms}",
      "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "applications": {
    "gecko": {
      "id": "Converter_Suite_fqRObEsLaM@convertersuite.com"
    }
  }
}