{
  "name": "Dokobit e-signing and e-identification",
  "version": "1.1.41",
  "minimum_chrome_version": "40.0",
  "manifest_version": 2,
  "description": "Easily and securely sign legally binding documents on Dokobit e-signing portal and login to various e-services of Dokobit partners",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*",
        "file:///*"
      ],
      "js": [
        "isign-content.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "isign-background.js"
    ]
  },
  "web_accessible_resources": [
    "isign-page.js"
  ],
  "permissions": [
    "nativeMessaging",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{055dcb74-b57b-4326-b033-bcf1446cb5c2}"
    }
  }
}