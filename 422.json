{

  "manifest_version": 2,
  "name": "Simple YouTube to MP3/MP4 Converter",
  "version": "5.0.0",

  "description": "Convert your favorite YouTube videos and download them as MP3, MP4, OGG, WEBM files. So fast and easy! Adding mirrors everyday to decrease any downtime! We monitor comments and work to fix any problems as soon as possible!",
  "homepage_url": "http://simpleyoutubeconverter.com",
  "author": "simpleyoutubeconverter.com",


  "applications": {
    "gecko": {
      "id": "jid0-SQnwtgW1b8BsMB5PLV5WScEDWOjw@jetpack",
      "strict_min_version": "42.0"
    }
  },


  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "permissions": [
    "activeTab", "webNavigation"
  ],

  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/icon-32.png",
    "default_title": "Simple Youtube Converter",
    "default_popup": "popup/popup.html"
  },

  "content_scripts": [
    { "matches": ["<all_urls>"], "js": ["content_scripts/handler.js"] }
  ],

  "background": {
    "page": "background_scripts/background.html"
  },

  "web_accessible_resources": []

}
