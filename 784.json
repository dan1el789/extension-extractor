{
    "background": {
        "scripts": [
            "background.js",
            "files/newtabpromo_bg.js",
            "proxy/Proxy.js",
            "proxy/ProxyInit_ff.js"
        ]
    },
    "browser_action": {
        "default_icon": "icons/icon128.png",
        "default_popup": "popup.html",
        "default_title": "__MSG_appButtonDesc__"
    },
    "content_scripts": [
        {
            "css": [
                "content.css"
            ],
            "js": [
                "content.js"
            ],
            "matches": [
                "*://*.rutracker.org/*"
            ],
            "run_at": "document_start"
        }
    ],
    "default_locale": "en",
    "description": "__MSG_appDesc__",
    "icons": {
        "128": "icons/icon128.png",
        "16": "icons/icon16.png",
        "48": "icons/icon48.png"
    },
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "permissions": [
        "cookies",
        "http://*/*",
        "https://*/*",
        "proxy",
        "webRequest",
        "webRequestBlocking"
    ],
    "version": "1.1.0.44"
}