{
  "manifest_version": 2,
  "name": "TTV LOL PRO",
  "description": "TTV LOL PRO removes most livestream ads from Twitch.",
  "version": "2.1.1",
  "background": {
    "scripts": [
      "background.c3174cc2.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "128": "icon.2ef02956.png"
    },
    "default_title": "TTV LOL PRO",
    "default_popup": "menu.e6b29ca9.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{76ef94a4-e3d0-4c6f-961a-d38a429a332b}",
      "strict_min_version": "91.1.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*",
        "https://m.twitch.tv/*"
      ],
      "js": [
        "content.5a50cd70.js"
      ],
      "run_at": "document_start",
      "css": []
    }
  ],
  "icons": {
    "128": "icon.2ef02956.png"
  },
  "options_ui": {
    "browser_style": false,
    "open_in_tab": true,
    "page": "options/page.html"
  },
  "permissions": [
    "proxy",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*.ttvnw.net/*",
    "https://*.twitch.tv/*",
    "https://perfprod.com/ttvlolpro/telemetry"
  ],
  "web_accessible_resources": [
    "page.28afcbc4.js",
    "worker.9ecf97e6.js"
  ]
}