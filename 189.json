{
  "background": {
    "scripts": [
      "distJs/constants.js",
      "distJs/helper.js",
      "html/js/storageReplacer.js",
      "util.js",
      "background.js",
      "misc.js"
    ]
  },
  "name": "Unit Conversion Tab & Custom Web Search",
  "short_name": "Unit Conversion Tab",
  "icons": {
    "128": "icons/Unit_Conversion_Tab_Colored.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://unitconversiontab.com/*"
      ],
      "run_at": "document_start",
      "all_frames": false,
      "js": [
        "contentScript.js"
