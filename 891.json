{
  "manifest_version": 2,
  "version": "0.70",
  "applications": {
    "gecko": {
      "id": "images@wink.su",
      "strict_min_version": "57.0"
    }
  },
  "default_locale": "en",
  "name": "__MSG_name__",
  "short_name": "__MSG_name__",
  "description": "__MSG_description__",
  "author": "Wink",
  "icons": {
    "16": "icons/eye.png",
    "32": "icons/logo.png",
    "48": "icons/logo_48.png",
    "64": "icons/logo_64.png",
    "128": "icons/logo_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content/content.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "options_ui": {
    "chrome_style": false,
    "page": "options/options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "activeTab",
    "notifications",
    "<all_urls>"
  ]
}