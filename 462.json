{
  "name": "__MSG_appName__",
  "version": "2023.4.1",
  "author": "Lean Library",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "20": "images/icon-20.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "options_ui": {
    "page": "options.html"
  },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "unlimitedStorage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-disabled-19.png",
      "20": "images/icon-disabled-20.png",
      "38": "images/icon-disabled-38.png"
    },
    "default_title": "Lean Library",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+L",
        "mac": "Alt+Shift+L"
      }
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "scripts/content-script.js"
      ],
      "css": [
        "styles/workflow.css",
        "styles/toastify.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/*",
    "content-popup.html",
    "scripts/*.js",
    "scripts/*.js.map",
    "styles/*.css",
    "vendor/scite-badge/dist/badge.bundle.js"
  ],
  "applications": {
    "gecko": {
      "id": "{809ea8a3-a45d-41a2-9cb0-e7c7d7321db5}"
    }
  },
  "-ms-preload": {
    "backgroundScript": "edge/backgroundScriptsAPIBridge.js",
    "contentScript": "edge/contentScriptsAPIBridge.js"
  }
}