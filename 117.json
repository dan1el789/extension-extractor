{
  "manifest_version": 2,
  "name": "Tab Reloader (page auto refresh)",
  "description": "__MSG_description__",
  "default_locale": "en",
  "version": "0.6.1",
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "webNavigation",
    "contextMenus",
    "idle"
  ],
  "background": {
    "scripts": [
      "/v2/chrome.action.js",
      "/v2/chrome.alarms.get.js",
      "/v2/chrome.alarms.getAll.js",
      "/v2/chrome.contextMenus.create.js",
      "/v2/chrome.permissions.contains.js",
      "/v2/chrome.permissions.request.js",
      "/v2/chrome.runtime.js",
      "/v2/chrome.scripting.js",
      "/v2/chrome.tabs.get.js",
      "/v2/chrome.tabs.query.js",
      "/v2/chrome.windows.get.js",
      "api.js",
      "defaults.js",
      "reload.js",
      "context.js",
      "worker.js"
    ]
  },
  "homepage_url": "https://webextension.org/listing/tab-reloader.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "open_in_tab": true
  },
  "commands": {
    "reload.all": {
      "description": "Reload all tabs"
    },
    "reload.all.discarded": {
      "description": "Reload all discarded tabs"
    },
    "reload.window": {
      "description": "Reload all tabs in the current window"
    },
    "reload.window.discarded": {
      "description": "Reload all discarded tabs in the current window"
    },
    "reload.tabs.left": {
      "description": "Reload tabs to the left"
    },
    "reload.tabs.right": {
      "description": "Reload tabs to the right"
    },
    "stop.all": {
      "description": "Stop all Reloading Jobs"
    },
    "_execute_browser_action": {}
  },
  "optional_permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "data/icons/disabled/16.png",
      "32": "data/icons/disabled/32.png"
    },
    "default_popup": "data/popup/index.html",
    "default_title": "Tab Reloader"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid0-bnmfwWw2w2w4e4edvcdDbnMhdVg@jetpack"
    }
  }
}