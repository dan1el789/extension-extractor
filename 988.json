{
  "name": "Secret Server Clipboard Utility",
  "manifest_version": 2,
  "version": "3.0.2",
  "permissions": [ "clipboardRead", "clipboardWrite", "tabs" ],
  "description": "Clipboard helper for Secret Server",
  "background": {
    "page": "background.html"
  },
  "homepage_url": "http://www.thycotic.com/products_secretserver_support.html",
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "script.js" ]
    }
  ],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "applications": {
    "gecko": {
      "id": "{db2e25d7-d802-4f1d-a909-f5540b5cd31c}",
      "strict_min_version": "56.0"
    }
  }
}