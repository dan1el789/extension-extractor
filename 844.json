{
    "background": {
        "scripts": [ "scripts/jquery-3.2.1.min.js", "scripts/msg.js", "background.js" ]
    },
    "content_scripts": [
        {
            "all_frames": true,
            "js": [ "scripts/jquery-3.2.1.min.js", "scripts/msg.js", "scripts/client.js" ],
            "matches": [ "http://*/*", "https://*/*" ]
        },
        {
            "js": [ "scripts/youtube.js" ],
            "matches": [ "http://www.downloadytb.com/*" ]
        }
    ],
    "default_locale": "en",
    "description": "__MSG_description__",
    "icons": {
        "128": "down.png",
        "16": "down.png",
        "32": "down.png",
        "48": "down.png"
    },
    "manifest_version": 2,
    "name": "__MSG_name__",
    "permissions": [ "https://*/*", "http://*/*", "menus" ],
    "version": "1.0.2"
}
