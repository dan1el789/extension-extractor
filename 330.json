{
  "manifest_version": 2,
  "name": "Wikiwand: Wikipedia Modernized",
  "short_name": "Wikiwand",
  "description": "Good old Wikipedia gets a great new look",
  "version": "5.1.3",
  "homepage_url": "https://www.wikiwand.com",
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "jid1-D7momAzRw417Ag@jetpack",
      "strict_min_version": "57.0"
    }
  },
  "permissions": [
    "cookies",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "*://*.wikipedia.org/*",
    "*://*.wikiwand.com/*"
  ],
  "background": {
    "scripts": [
      "js/namespaces.js",
      "js/url-tests.js",
      "js/background.js"
    ]
  },
  "icons": {
    "128": "images/icon128.png"
  }
}
