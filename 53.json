{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  // NOTE: update this in search_url as well
  "version": "1.3.0",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  },
  "icons": {
    "64": "icons/sp-icon.png"
  },
  "permissions": [
    "search",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*.startpage.com/*"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_searchEngineName__",
      "search_url": "https://www.startpage.com/do/dsearch?query={searchTerms}&cat=web&pl=ext-ff&language=__MSG_extensionUrlLanguage__&extVersion=1.3.0",
      "favicon_url": "https://www.startpage.com/favicon.ico",
      "is_default": true
    }
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.startpage.com/*"],
      "js": ["js/content.js"],
      "css": ["css/content.css"],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en"
}
