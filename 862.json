{
  "name": "Open in Tor Browser",
  "short_name": "ioitor",
  "description": "Open current page or link in tor browser for better privacy",
  "author": "Andy Portmen",
  "version": "0.1.1",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "nativeMessaging",
    "downloads",
    "https://api.github.com/repos/andy-portmen/native-client/releases/latest"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "homepage_url": "http://add0n.com/open-in.html?from=tor",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["data/inject.js"],
    "run_at": "document_start",
    "all_frames": true
  }],
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "applications": {
    "gecko": {
      "id": "{9d3b260b-886d-4263-b9d6-81d756ee4929}"
    }
  }
}
