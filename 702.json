{
  "manifest_version": 2,
  "name": "__MSG_application_title__",
  "description": "__MSG_application_description__",
  "default_locale": "en",
  "version": "4.36.0",
  "browser_action": {
    "default_icon": "data/images/addon_icon_32.png",
    "default_title": "__MSG_application_title__",
    "default_popup": "data/templates/buyee.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "data/css/buyee.css"
      ],
      "js": [
        "data/js/background/content_script.js"
      ]
    }
  ],
  "icons": {
    "32": "data/images/addon_icon_32.png",
    "64": "data/images/addon_icon_64.png",
    "128": "data/images/addon_icon_128.png"
  },
  "web_accessible_resources": [
    "data/images/logo_buyee.png",
    "data/images/buyeeicon-cart_white.svg",
    "data/images/buyeeicon-cart_blue.svg",
    "data/images/buyeeicon-credit_blue.svg",
    "data/images/buyeeicon-close.svg",
    "data/images/buyee_close.svg",
    "data/images/buyee_solid_arrow.svg",
    "data/images/en/buyee_banner.png",
    "data/images/ja/buyee_banner.png",
    "data/images/chs/buyee_banner.png",
    "data/images/cht/buyee_banner.png",
    "data/images/th/buyee_banner.png",
    "data/images/id/buyee_banner.png",
    "data/images/ko/buyee_banner.png",
    "data/images/de/buyee_banner.png",
    "data/images/es/buyee_banner.png",
    "data/images/ru/buyee_banner.png"
  ],
  "background": {
    "scripts": [
      "data/js/handler.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://*.buyee.jp/*",
    "https://ip-country.connect.buyee.jp/*",
    "activeTab",
    "tabs",
    "storage"
  ],
  "applications": {
    "gecko": {
      "id": "@buyee-cart-extension",
      "strict_min_version": "90.0"
    }
  }
}