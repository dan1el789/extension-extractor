{
	"name": "YouTube Downloader",

	"developer": {
		"name": "ugafan6",
		"url": "https://loader.to"
	},

	"description": "The fastest and easiest YouTube Downloader on the Internet! All major formats supported! Completely free!",
	"manifest_version": 2,
	"version": "1.4",

	"icons": {
		"48": "icons/logo_64x64.png"
	},

	"content_scripts": [{
		"js": ["includes/youtube-downloader.js"],
		"matches": ["http://www.youtube.com/*", "https://www.youtube.com/*", "*://*.youtube.com/*", "*://youtube.com/*", "*://youtu.be/*"],
		"exclude_matches": ["http://www.youtube.com/embed/*", "https://www.youtube.com/embed/*"],
		"run_at": "document_start"
	}],

	"web_accessible_resources": ["icons/logo_64x64.png"],
	"permissions": ["<all_urls>"],
	"incognito": "spanning"
}