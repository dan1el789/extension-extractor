{
  "manifest_version": 2,
  "name": "K7 Webprotection",
  "version": "6.3",
  "author": "K7 Computing",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/K7Logo_48.png",
    "default_title": "K7 Webprotection"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "k7srff_enUS@k7computing.com",
      "strict_min_version": "56.0"
    }
  },
  "options_ui": {
    "page": "privacy.html",
    "browser_style": false,
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "boot.js",
        "privacy.js"
      ]
    }
  ],
  "description": "",
  "icons": {
    "18": "images/K7Logo_48.png"
  },
  "permissions": [
    "storage",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "images/k7ano_en.gif",
    "images/Prompt_Background.png",
    "images/ToolTip_Background.png",
    "images/K7Logo.png",
    "images/incorrect.png",
    "images/correct.png",
    "images/cls.png",
    "images/i1.png",
    "images/i2.png",
    "images/i3.png",
    "images/al.png",
    "images/ar.png",
    "images/ToolTip_GreenStip.png",
    "images/ToolTip_RedStrip.png",
    "images/verisign.gif",
    "images/ToolTip_Arrow.png",
    "images/right_arrow.png"
  ]
}