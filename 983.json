{
  "manifest_version": 2,
  "name": "Bionic Reader",
  "version": "1.6.23",
  "description": "Apply Bionic Reading style to a webpage.",
  "icons": {
    "48": "icons/48x48.png",
    "96": "icons/96x96.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_action": {
    "browser_style": true,
    "default_popup": "options.html",
    "default_icon": "icons/32x32.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/utls.js",
        "content/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{c2ecdf60-7077-4bfa-b9c2-4892a8ded8c6}",
      "strict_min_version": "100.0"
    }
  }
}