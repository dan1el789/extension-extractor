{
  "manifest_version": 2,
  "author": "Savevur",
  "version": "1.2.53",
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "content_security_policy": "default-src 'none'; sandbox;",
  "icons": {
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "96": "icon/icon-96.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png",
      "96": "icon/icon-96.png"
    }
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "__MSG_searchName__",
      "favicon_url": "icon/icon-320.png",
      "search_form": "https://yandex.__MSG_searchDomainZone__/",
      "search_url": "https://yandex.__MSG_searchDomainZone__/search",
      "search_url_get_params": "text={searchTerms}",
      "suggest_url": "https://suggest.yandex.__MSG_searchDomainZone__/suggest-ff.cgi",
      "suggest_url_get_params": "part={searchTerms}",
      "keyword": [
        "0",
        "\u044f",
        "z"
      ],
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://yandex.by/*",
        "*://yandex.com/*",
        "*://yandex.com.tr/*",
        "*://yandex.eu/*",
        "*://yandex.kz/*",
        "*://yandex.ru/*"
      ],
      "js": [
        "opensearch.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "*://yandex.by/*",
        "*://yandex.com/*",
        "*://yandex.com.tr/*",
        "*://yandex.eu/*",
        "*://yandex.kz/*",
        "*://yandex.ru/*"
      ],
      "js": [
        "favicon.js",
        "base_title.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "icon/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{f479fa23-7074-4c16-bca2-4fae263f7e5a}"
    }
  }
}