{
   "applications": {
      "gecko": {
         "id": "michal.simonfy@gmail.com",
         "strict_min_version": "54.0"
      }
   },
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icon64.png",
      "default_popup": "menu.html",
      "default_title": "Speed Dial 2"
   },
   "chrome_url_overrides": {
      "newtab": "newtab.html"
   },
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "homepage_url": "http://speeddial2.com",
   "icons": {
      "128": "icon128.png",
      "64": "icon64.png",
      "48": "icon48.png",
      "32": "icon32.png",
      "16": "icon16.png"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "short_name": "__MSG_appShortName__",
   "permissions": [ "bookmarks", "topSites", "tabs", "history", "storage", "unlimitedStorage", "<all_urls>", "alarms" ],
   "version": "2.4.7",
   "web_accessible_resources": [ "newtab.html" ]
}
