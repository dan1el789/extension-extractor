{
	"manifest_version": 2,
	"background": {
		"scripts": [
			"js/jquery-3.2.1.min.js",
			"js/erailchrome.js"
			
		]
	},
	"browser_action": {
		"default_icon": {
			"16": "images/erail.png",
			"48": "images/erail48.png",
			"128": "images/erail128.png"
		},
		"default_popup": "popup.html",
		"default_title": "eRail.in"
	},
	"content_scripts": [
		{
			"js": [
				"js/jquery-3.2.1.min.js",
				"js/contentscript.js",
				"js/ocrad.js"
			],
			"matches": [
				"*://*.erail.in/*",
				"*://*.eair.in/*",
				"*://*.irctc.co.in/*",
				"*://*.indianrail.gov.in/*",
				"*://*.makemytrip.com/*",
				"*://*.paytm.com/*"
			]
		}
	],
	"description": "This app provides enhanced user experience for eRail.in",
	"icons": {
		"16": "images/erail.png",
		"48": "images/erail48.png",
		"128": "images/erail128.png"
	},
	"name": "eRail.in",
	"short_name": "eRail.in",
	"omnibox": {
		"keyword": "erailir"
	},
	"permissions": [
		"cookies",
		"webRequest",
		"webRequestBlocking",
		"tabs",
		"management",
		"contextMenus",
		"notifications",
		"unlimitedStorage",
		"*://*.erail.in/*",
		"*://*.eair.in/*",
		"*://*.irctc.co.in/*",
		"*://*.indianrail.gov.in/*",
		"*://*.makemytrip.com/*",
		"*://*.paytm.com/*"
	],
	"version": "6.9"
}