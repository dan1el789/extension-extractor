{
  "applications": {
    "gecko": {
      "id": "kcnofmiceklfkodhdhhjcfjhdepfobaf@unifiedpost.com",
      "strict_min_version": "50.0"
    }
  },
  "name": "eidReader Plugin Extension",
  "version": "1.0.2.0amo1",
  "manifest_version": 2,
  "description": "Allows use of native eidReader plugin",
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "scripts": ["background.js", "FireBreathPromise.js", "wyrmhole.js", "firewyrm.js"]
  },
  "content_scripts": [{
      "matches": [ "<all_urls>" ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
  }],
  "web_accessible_resources": [
      "wyrmhole.js",
      "wyrmhole-page.js",
      "FireBreathPromise.js",
      "firewyrm.js"
  ],
  "permissions": [
    "nativeMessaging"
  ]
}
