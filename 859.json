{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "incognito": "spanning",
  "homepage_url": "https://www.vpnunlimited.com",
  "permissions": [
    "proxy",
    "tabs",
    "storage",
    "<all_urls>",
    "webRequest",
    "privacy",
    "webRequestBlocking"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/app_icon_connected_16.png",
    "48": "icons/app_icon_connected_48.png",
    "128": "icons/app_icon_connected_128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_title__",
    "default_icon": {
      "16": "icons/app_icon_error_16.png",
      "48": "icons/app_icon_error_48.png",
      "128": "icons/app_icon_error_128.png"
    }
  },
  "web_accessible_resources": [
    "icons/*",
    "images/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "@vpn-unlimited-secure-proxy",
      "strict_min_version": "91.1.0"
    }
  },
  "version": "9.0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}