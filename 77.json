{
  "manifest_version": 2,
  "name": "NordVPN - A VPN Proxy Extension for Firefox",
  "short_name": "Web Proxy",
  "description": "With this proxy extension, you can stay secure and private on the Internet and avoid those annoying online ads.",
  "icons": {
    "16": "./icons/nordvpn-16-active.png",
    "32": "./icons/nordvpn-32-active.png",
    "48": "./icons/nordvpn-48-active.png",
    "128": "./icons/nordvpn-128-active.png"
  },
  "browser_action": {
    "default_icon": "icons/nordvpn-48-inactive.png",
    "default_popup": "index.html",
    "default_title": "NordVPN Browser Extension"
  },
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestBlocking",
    "privacy",
    "<all_urls>",
    "storage",
    "notifications",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.bundle.js",
      "moose.bundle.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "contentScript.bundle.css"
  ],
  "version": "3.1.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "nordvpnproxy@nordvpn.com",
      "strict_min_version": "92.0"
    }
  },
  "content_security_policy": "script-src 'self' 'wasm-unsafe-eval' ; object-src 'self'"
}