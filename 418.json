{  
   "name":"__MSG_extName__",
   "description":"__MSG_extDescription__",
   "default_locale": "en",
   "version":"1.4",
   "applications":{  
      "gecko":{  
        "id":"antitrack@avg.com"
      }
   },
   "icons":{
      "16": "images/avg16.png",
      "24": "images/avg24.png",
		"32": "images/avg32.png",
		"48": "images/avg48.png",
		"64": "images/avg64.png",
		"128": "images/avg128.png"
   },
   "manifest_version":2,
   "content_scripts":[  
      {  
         "run_at":"document_start",
         "js":[  
            "contentscript.js"
         ],
         "matches":[  
            "<all_urls>"
         ]
      }
   ],
   "permissions":[
      "tabs",
      "nativeMessaging",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
   ],
   "background":{  
      "scripts":[  
         "main.js"
      ]
   }
}