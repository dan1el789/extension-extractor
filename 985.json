{
  "manifest_version": 2,
  "name": "Youtube to MP3 Converter",
  "version": "1.3.1",
  "description": "Firefox extension to convert and download Youtube videos for free in MP3 format.",
  "homepage_url": "https://mp3convert.org",
  "permissions": [
    "http://*.youtube.com/*"
  ],
  "icons": {
    "64": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "download.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{32a0ec69-a3af-4f43-a18d-6a46637fe412}"
    }
  }
}