{
  "manifest_version": 2,
  "name": "Keplr",
  "description": "Keplr is a browser extension wallet for the Inter blockchain ecosystem.",
  "version": "0.12.10",
  "icons": {
    "16": "assets/icon-beta-16.png",
    "48": "assets/icon-beta-48.png",
    "128": "assets/icon-beta-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Keplr"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "storage",
    "notifications",
    "identity",
    "idle",
    "alarms"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "browser-polyfill.js",
        "contentScripts.bundle.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    "injectedScript.bundle.js",
    "blocklist.html"
  ],
  "applications": {
    "gecko": {
      "id": "keplr-extension@keplr.app",
      "strict_min_version": "89.0"
    }
  }
}