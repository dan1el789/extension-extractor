{
  "browser_specific_settings": {
    "gecko": {
      "id": "checkerplusforgmail@jasonsavard.com"
    }
  },
  "name": "Checker Plus for Gmail",
  "description": "__MSG_description__",
  "version": "24.1.1.1",
  "homepage_url": "https://jasonsavard.com?ref=homepage_url&ext=gmail",
  "background": {
    "scripts": [
      "js/lib/Autolinker.js",
      "js/common.js",
      "js/checkerPlusForGmail.js",
      "js/mime/addressparser.js",
      "js/mime/buffer.js",
      "js/mime/iconv.js",
      "js/mime/encoding.js",
      "js/mime/mimelib.js",
      "js/account.js",
      "js/mail.js",
      "js/sanitizer/html4.js",
      "js/sanitizer/uri.js",
      "js/sanitizer/sanitizer.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/browserButtons/default/not_logged_in.png",
      "38": "images/browserButtons/default/not_logged_in_retina.png"
    },
    "default_title": "Checker Plus for Gmail\u2122",
    "default_popup": "popup.html?source=toolbar&setVia=manifest"
  },
  "icons": {
    "16": "images/icons/icon_16.png",
    "32": "images/icons/icon_32.png",
    "48": "images/icons/icon_48.png",
    "64": "images/icons/icon_64.png",
    "128": "images/icons/icon_128.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "js/tabletView.js"
      ],
      "matches": [
        "https://mail.google.com/mail/mu/mp/*"
      ],
      "run_at": "document_start"
    }
  ],
  "default_locale": "en",
  "permissions": [
    "alarms",
    "activeTab",
    "tabs",
    "tts",
    "idle",
    "notifications",
    "storage",
    "contextMenus",
    "https://mail.google.com/",
    "https://gmail.googleapis.com/*"
  ],
  "optional_permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.googleusercontent.com/"
  ],
  "storage": {
    "managed_schema": "schema.json"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "options.html",
    "contribute.html",
    "oauth2callback.html",
    "images/icons/icon_48.png",
    "css/tabletView.css"
  ]
}