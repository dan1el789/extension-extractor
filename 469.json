{
  "description": "\u00a9 Thales Group 2023",
  "web_accessible_resources": [
    "templates/*",
    "assets/*"
  ],
  "name": "Gemalto Web Signer for Barclays",
  "content_scripts": [
    {
      "matches": [
        "https://europe.tradeonlineservices.com/*",
        "https://*.barclays.com/*",
        "https://*.barclays.net/*",
        "https://*.barclays.intranet/*",
        "https://*.barclays.co.uk/*",
        "https://*.barclayswealth.com/*",
        "https://*.barclayscorp.com/*",
        "https://*.barcapint.com/*",
        "https://*.bmap.barclays/*",
        "https://*.barclays-sepadirect.com/*",
        "https://*.cashfaccloud.co.uk/*",
        "https://*.bacs.co.uk/*",
        "https://*.voca.com/*",
        "https://*.voca.co.uk/*",
        "https://*.identrust.com/*",
        "https://*.paygateservice.com/*",
        "https://*.iconnect.aptbacs.co.uk/*",
        "https://*.interpayweb.interbacs.com/*",
        "https://*.fm.interpayweb.co.uk/*",
        "https://*.bacscom.mosaicsoftware.co.uk/*"
      ],
      "all_frames": true,
      "js": [
        "first.js",
        "jquery.js",
        "custom.js",
        "app_settings.js",
        "modals.js",
        "utility.js",
        "multiPageCtrl.js",
        "certificateTable.js",
        "customScrollbars.js",
        "verificationResults.js",
        "asn1.js",
        "version.js",
        "logger.js",
        "content.js"
      ],
      "css": [
        "assets/css/main.css"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "icons": {
    "128": "assets/img/logo128.png",
    "48": "assets/img/logo48.png",
    "16": "assets/img/logo16.png"
  },
  "applications": {
    "gecko": {
      "strict_min_version": "57.0",
      "id": "websigner_barclays_live@gemalto"
    }
  },
  "version": "1.8.0.5",
  "options_ui": {
    "page": "diagTools/diagTools.html",
    "browser_style": false
  },
  "background": {
    "persistent": false,
    "scripts": [
      "base64.js",
      "rasterizeHTML.js",
      "DOMPurify.js",
      "app_settings.js",
      "renderer.js",
      "logger.js",
      "utility.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "nativeMessaging",
    "storage",
    "unlimitedStorage"
  ]
}