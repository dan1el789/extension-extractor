{
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "icons": {
    "128": "img/128.png",
    "64": "img/64.png",
    "32": "img/32.png"
  },
  "browser_action": {
    "default_icon": "img/32.png",
    "default_title": "__MSG_title__",
    "default_popup": "/popup.html"
  },
  "background": {
    "scripts": [
      "/js/libs/bundle.min.js",
      "/js/libs/jquery.min.js",
      "/js/storage.js",
      "/js/app.js"
    ]
  },
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*",
    "management",
    "notifications",
    "proxy",
    "storage",
    "tabs"
  ],
  "version": "6.0.3"
}
