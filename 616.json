{
  "name": "遊戲橘子擴充元件",
  "version": "2.3.11",
  "manifest_version": 2,
  "description": "This extension is used to check if user installed WebStartInit.msi or not.",
  "icons":
  {
    "128": "beanfun!.png"
  },
  "applications": {
    "gecko": {
      "id": "gtwitpt@gmail.com",
      "strict_min_version": "50.0"
    }
  },
  "content_scripts":
  [{
		"matches": ["*://*/*"],
        "js": [ "check.js" ],
		"run_at": "document_end"
  }],
  "permissions":
  [
    "nativeMessaging",
    "*://*/*"
  ],
  "background":
  {
    "scripts":[ "contactclient.js" ]
  }
}
