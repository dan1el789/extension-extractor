{
	"manifest_version": 2,
	"name": "__MSG_name__",
	"description": "__MSG_description__",
	"version": "2.0.0",
	"applications": {
		"gecko": {
			"id": "google-page-translator-en-ja@id.pokemori.jp"
		}
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "google.png"
	},
	"options_ui": {
		"browser_style": true,
		"page": "options.xhtml"
	},
	"default_locale": "en",
	"icons": {
		"16": "google.png"
	},
	"author": "100の人"
}
