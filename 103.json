            "js/background/notification-maker.class.js",
            "js/background/downloads-folder-query.class.js",
            "js/background/background.js"
        ]
    },

    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "run_at": "document_start",
            "js": [
                "js/libs/purify.min.js",
