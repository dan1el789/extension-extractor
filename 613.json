{
  "manifest_version": 2,
  "version": "3.25.3",
  "name": "daily.dev | The homepage developers deserve",
  "short_name": "daily.dev",
  "description": "Get one personalized feed for all the knowledge you need as a developer.",
  "homepage_url": "https://daily.dev",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "https://daily.dev/",
    "https://*.daily.dev/",
    "https://dailynow.co/",
    "https://*.dailynow.co/"
  ],
  "optional_permissions": [
    "topSites",
    "*://*/*"
  ],
  "content_security_policy": "object-src 'self'",
  "browser_action": {
    "default_icon": {
      "16": "icons/action_16.png",
      "32": "icons/action_32.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.bundle.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    "index.html",
    "companion.html",
    "js/companion.bundle.js",
    "css/companion.css"
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "54.0",
      "id": "{504e1565-ddd7-4fc3-bec3-079ddb622b2c}"
    }
  }
}