{
  "manifest_version": 2,
  "name": "Web Threat Shield",
  "version": "2.1.22286.1",
  "default_locale": "en",
  "description": "__MSG_description__",
  "author": "Webroot Inc.",
  "applications": {
    "gecko": {
      "id": "webrootsecure@webroot.com"
    }
  },
  "developer": {
    "name": "Webroot",
    "url": "https://www.webroot.com"
  },
  "background": {
    "page": "background_scripts/background.html",
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "content_scripts/sra.css"
      ],
      "js": [
        "helper_scripts/helper.js",
        "helper_scripts/browser.js",
        "content_scripts/main.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "downloads",
    "webRequest",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "16": "images/Webroot16.png",
    "24": "images/Webroot24.png",
    "32": "images/Webroot32.png",
    "48": "images/Webroot48.png",
    "96": "images/Webroot96.png",
    "128": "images/Webroot128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/Webroot16.png",
      "24": "images/Webroot24.png",
      "32": "images/Webroot32.png",
      "48": "images/Webroot48.png",
      "96": "images/Webroot96.png",
      "128": "images/Webroot128.png"
    },
    "default_title": "Web Threat Shield",
    "default_popup": "browser_actions/keycode_ui/keycode_ui.html"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options/options.html"
  },
  "content_security_policy": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "images/sra/GoSm.svg",
    "images/sra/YieldSm.svg",
    "images/sra/StopSm.svg",
    "images/sra/WebrootSmall.svg",
    "content_scripts/sra.css",
    "images/fonts/OpenSans_300_400_600_700.woff2"
  ]
}