      ],
      "css": [
        "player.css"
      ],
      "js": [
        "parse.js",
        "content.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "http://*.pbion.com/*",
        "https://*.pbion.com/*"
      ],
      "js": [
        "pbion.js"
      ]
    }
  ],
  "browser_action": {
