{
  "manifest_version": 2,
  "name": "Font Finder",
  "version": "0.5.6",
  "description": "__MSG_app_description__",
  "default_locale": "en",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "/v2/chrome.contextMenus.create.js",
      "/v2/chrome.runtime.js",
      "/v2/chrome.scripting.js",
      "/v2/chrome.action.js",
      "/v2/chrome.windows.create.js",
      "/v2/chrome.windows.get.js",
      "/v2/chrome.windows.getCurrent.js",
      "context.js",
      "worker.js"
    ]
  },
  "homepage_url": "https://webextension.org/listing/font-finder.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png"
  },
  "web_accessible_resources": [
    "data/inject/AND-Regular.otf",
    "data/window/index.html"
  ],
  "options_ui": {
    "page": "data/options/index.html"
  },
  "commands": {
    "_execute_browser_action": {}
  },
  "optional_permissions": [
    "<all_urls>"
  ],
  "browser_action": {},
  "browser_specific_settings": {
    "gecko": {
      "id": "{a658a273-612e-489e-b4f1-5344e672f4f5}"
    }
  }
