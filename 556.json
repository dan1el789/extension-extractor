{
  "manifest_version": 2,
  "name": "CNET Shopping",
  "short_name": "CNET Shopping",
  "version": "12.7",
  "description": "CNET Shopping helps you get the best prices on your favorite products. Get promo codes and discounts with a single click.",
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "webNavigation",
    "webRequest",
    "storage",
    "alarms",
    "cookies"
  ],
  "background": {
    "scripts": [
      "vendor/lodash.min.js",
      "vendor/purify.min.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "vendor/jquery.min.js",
        "vendor/lodash.min.js",
        "vendor/purify.min.js",
        "js/autocoupons.js",
        "js/main.js",
        "js/autoapplycoupons.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "html/popup.html",
    "css/popup.css",
    "js/popup.js",
    "fonts/*",
    "images/*",
    "html/*",
    "js/*",
    "css/*",
    "vendor/*"
  ],
  "browser_action": {
    "default_icon": "images/cnet-logo-128.png",
    "default_title": "CNET Shopping"
  },
  "icons": {
    "16": "images/cnet-logo-16.png",
    "48": "images/cnet-logo-48.png",
    "128": "images/cnet-logo-128.png"
  },
  "options_ui": {
    "page": "html/options.html"
  },
  "applications": {
    "gecko": {
      "id": "canitbecheaper@trafficbroker.co.uk",
      "strict_min_version": "51.0a1"
    }
  }
}