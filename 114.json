{
	"manifest_version": 2,
	"version": "0.9.22",
	"name": "РуТрекер - официальный плагин (доступ и пр.)",
	"short_name": "РуТрекер",
	"description": "РуТрекер: комфортная работа с зеркалами, поиск, доступ к RuTracker.org и пр.",
	"background": {
		"page": "/background.html"
	},
	"icons": {
		"128": "/images/addon-128.png"
	},
	"browser_action": {
		"default_icon": "/images/addon-128.png",
		"default_title": "РуТрекер (официальный плагин)",
		"default_popup": "/pages/popup.html"
	},
	"options_ui": {
		"page": "/pages/options.html",
		"chrome_style": false,
		"open_in_tab": true
	},
	"web_accessible_resources": [
		"/pages/*"
	],
	"permissions": [
		"<all_urls>",
		"activeTab",
		"contextMenus",
		"proxy",
		"storage",
		"tabs",
		"webRequest",
		"webRequestBlocking"
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "rto@rto.rto",
			"strict_min_version": "56.0"
		}
	}
}