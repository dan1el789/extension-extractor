{

  "manifest_version": 2,
  "name": "HD Youtube Downloader",
  "description": "Download youtube videos in highest available qulaity.",
  "background": {
    "scripts": ["background.js"]
  },
  "version": "3.4",
  "applications": {
      "gecko": {
        "id": "hd-youtube-downloader-toolbarteam101@gmail.com",
        "strict_min_version": "42.0"
    }
  },
     "icons": { 
          "128":"images/icon.png" 
  }, 

  "browser_action": {
   "default_icon": "images/icon.png"
  },
  "permissions": [
    "tabs", "storage", "downloads","webRequest","webRequestBlocking", "webNavigation", "*://youtube.com/*", "*://*.youtube.com/*", "*://*.googlevideo.com/*"
   ],
"content_scripts": [{
    "js" : ["jquery-3.2.1.min.js","content.js"],
    "css": ["style.css"],
    "matches": ["*://youtube.com/*", "*://*.youtube.com/*"],
    "run_at": "document_end"
  }],

"web_accessible_resources": [
    "images/*.png",
    "style.css"
  ]
}