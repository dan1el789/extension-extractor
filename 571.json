{
  "manifest_version": 2,
  "name": "Download Facebook Video or Photo",
  "version": "1.0.3",
  "description": "Click to download any Facebook video or photo.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "*://*.1clickfb.com/"
  ]
}