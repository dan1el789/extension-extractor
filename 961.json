{
  "manifest_version": 2,
  "name": "Rotate Youtube Video",
  "version": "5.4.7",
  "description": "Toolbar button for easier Rotate, Zoom or Mirror Youtube video",
  "icons": {
    "48": "icons/logo_48.png",
    "64": "icons/logo_64.png",
    "96": "icons/logo_96.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "css": [
        "content-script/main.css"
      ],
      "js": [
        "core-share.js",
        "utils.js",
        "content-script/variables.js",
        "content-script/pre.js",
        "content-script/main.js"
      ]
    }
  ],
  "commands": {
    "rotate": {
      "suggested_key": {
        "default": "Ctrl+Shift+9",
        "mac": "MacCtrl+Shift+9"
      },
      "description": "Rotate"
    },
    "zoom_in": {
      "suggested_key": {
        "default": "Ctrl+Shift+Period",
        "mac": "MacCtrl+Shift+Period"
      },
      "description": "Zoom in"
    },
    "zoom_out": {
      "suggested_key": {
        "default": "Ctrl+Shift+Comma",
        "mac": "MacCtrl+Shift+Comma"
      },
      "description": "Zoom out"
    },
    "move_left": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "MacCtrl+Shift+Left"
      },
      "description": "Move left"
    },
    "move_right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "MacCtrl+Shift+Right"
      },
      "description": "Move right"
    },
    "move_up": {
      "suggested_key": {
        "default": "Ctrl+Shift+Up",
        "mac": "MacCtrl+Shift+Up"
      },
      "description": "Move up"
    },
    "move_down": {
      "suggested_key": {
        "default": "Ctrl+Shift+Down",
        "mac": "MacCtrl+Shift+Down"
      },
      "description": "Move down"
    },
    "mirror": {
      "suggested_key": {
        "default": "Ctrl+Shift+8",
        "mac": "MacCtrl+Shift+8"
      },
      "description": "Mirror"
    },
    "reset": {
      "suggested_key": {
        "default": "Ctrl+Shift+7",
        "mac": "MacCtrl+Shift+7"
      },
      "description": "Reset Zoom / Move / Mirror Options"
    },
    "crop_move_in": {
      "suggested_key": {
        "default": "Ctrl+Shift+3",
        "mac": "MacCtrl+Shift+3"
      },
      "description": "Crop move in"
    },
    "crop_move_out": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "MacCtrl+Shift+2"
      },
      "description": "Crop move out"
    },
    "reset_crop": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "MacCtrl+Shift+1"
      },
      "description": "Reset crop"
    }
  },
  "permissions": [
    "tabs",
    "storage",
    "management"
  ],
  "options_ui": {
    "page": "options/index.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "web_accessible_resources": [
    "tensorflow/model/model.json",
    "tensorflow/model/group1-shard1of3.bin",
    "tensorflow/model/group1-shard2of3.bin",
    "tensorflow/model/group1-shard3of3.bin"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{075e8f87-f625-445c-926e-e2411df98fba}"
    }
  }
}