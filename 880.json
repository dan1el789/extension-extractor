{
  "version": "0.1.1",
  "manifest_version": 2,
  "name": "Reload All Tabs",
  "permissions": ["storage"],
  "short_name": "reload-all-tabs",
  "background": {"page": "lib/chrome/background.html"},
  "homepage_url": "https://mybrowseraddon.com/reload-all-tabs.html",
  "options_ui": {"chrome_style": true, "page": "data/options/options.html"},
  "description": "Reload all tabs using keyboard shortcut or toolbar button.",
  "commands": {
    "reload-hotkey": {
      "description": "Reload All Tabs",
      "suggested_key": {
        "mac": "Alt+Shift+C",
        "default": "Alt+Shift+C"
      }
    }
  },
  "browser_action": {
    "default_title": "Reload All Tabs",
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  }
}
