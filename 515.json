{
  "name": "__MSG_appName__",
  "version": "2.8.6",
  "manifest_version": 2,
  "default_locale": "en",
  "description": "__MSG_appDesc__",
  "applications": {
    "gecko": {
      "id": "{228a49ed-af0c-452c-bc77-630f99cb0470}"
    }
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup/popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "content_scripts": [
    {
      "match_about_blank": true,
      "exclude_matches": [
        "https://wuxian.taobao.com/decorate/*",
        "http://wuxian.taobao.com/decorate/*",
        "https://paper.dropbox.com/*"
      ],
      "js": [
        "js/utils/jquery.min.js",
        "js/utils/jquery-ui.min.js",
        "js/utils/element-helper.js",
        "js/utils/logger.js",
        "js/utils/mousetrap.min.js",
        "js/utils/extract-text.js",
        "js/utils/utils.js",
        "js/utils/url-helper.js",
        "js/utils/runtime-helper.js",
        "js/sieve.js",
        "js/utils/ajax-content.js",
        "js/content.js"
      ],
      "css": [
        "css/eagle-extension.css",
        "css/saveBoard.css"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "file:///*/*"
      ],
      "all_frames": false,
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://core.spgateway.com/MPG/mpg_gateway",
        "https://payment.ezpay.com.tw/MPG/mpg_gateway*",
        "https://payment.ezpay.com.tw/MPG/mpg_gateway/payment_info_submit"
      ],
      "js": [
        "js/utils/jquery.min.js",
        "plugins/alipay/alipay.js"
      ],
      "css": [
        "plugins/alipay/alipay.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://core.spgateway.com/API/Alipay",
        "https://core.spgateway.com/MPG/mpg_gateway/payment_info_submit"
      ],
      "css": [
        "plugins/alipay/alipay.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://payment.ezpay.com.tw/MPG/*"
      ],
      "js": [
        "js/utils/jquery.min.js",
        "plugins/ezpay/ezpay.js"
      ],
      "css": [
        "plugins/ezpay/ezpay.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.bing.com/images/*"
      ],
      "js": [
        "js/utils/jquery.min.js",
        "plugins/bing/bing.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.zcool.com.cn/*"
      ],
      "css": [
        "plugins/zcool/zcool.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": [
        "plugins/instagram/instagram.js"
      ],
      "css": [
        "plugins/instagram/instagram.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "js": [
        "plugins/twitter/twitter.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*.pinterest.com/*",
        "https://*.pinterest.com.au/*",
        "https://*.pinterest.jp/*",
        "https://*.pinterest.de/*",
        "https://*.pinterest.dk/*",
        "https://*.pinterest.se/*",
        "https://*.pinterest.pt/*",
        "https://*.pinterest.es/*",
        "https://*.pinterest.cl/*",
        "https://*.pinterest.fr/*",
        "https://*.pinterest.nz/*",
        "https://*.pinterest.co.kr/*",
        "https://*.pinterest.co.uk/*"
      ],
      "js": [
        "js/utils/jquery.min.js",
        "plugins/pinterest/pinterest.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*.behance.net/*"
      ],
      "css": [
        "plugins/behance/behance.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://weibo.com/*"
      ],
      "css": [
        "plugins/weibo/weibo.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://dribbble.com/*"
      ],
      "css": [
        "plugins/dribbble/dribbble.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://500px.com/*"
      ],
      "js": [
        "plugins/500px/500px.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.archdaily.cn/*",
        "https://www.archdaily.com/*"
      ],
      "css": [
        "plugins/archdaily/archdaily.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://apps.apple.com/*"
      ],
      "css": [
        "plugins/appstore/appstore.css"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "js/utils/jquery.min.js",
      "js/utils/logline.js",
      "js/utils/logger.js",
      "js/sieve.js",
      "js/background.js",
      "js/utils/ajax-background.js",
      "js/utils/utils.js",
      "js/api.js",
      "js/utils/cache-helper.js",
      "js/utils/url-helper.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "contextMenus",
    "storage",
    "http://*/*",
    "https://*/*",
    "tabs",
    "alarms"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}